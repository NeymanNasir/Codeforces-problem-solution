#include<iostream>
#include<cstdio>
#include<vector>
#include<algorithm>
using namespace std;

int main()
{
    int n, x, ans;
    vector<int> V;
    cin >> n;
    for(int i = 0; i < n;  i++)
    {
        cin >> x;
        V.push_back(x);
    }
    sort(V.rbegin(), V.rend());
    ans = V[n/2];

    cout << ans << endl;
    return 0;
}
