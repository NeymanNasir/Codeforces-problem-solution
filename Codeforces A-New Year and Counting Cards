#include<iostream>
#include<cstdio>
#include<string>
using namespace std;

int main()
{
    string str;
    cin >> str;

    int flip = 0;
    for(int i = 0; i < str.size(); i++)
    {
        if(str[i] == 'a' || str[i] == 'e' || str[i] =='i' || str[i] == 'o' || str[i] == 'u' || str[i] == '1' || str[i] == '3' || str[i] =='5' || str[i] == '7' || str[i] == '9' )
            flip++;
    }

    cout << flip << endl;
    return 0;
}
