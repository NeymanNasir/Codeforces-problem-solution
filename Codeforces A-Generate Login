#include<iostream>
#include<cstdio>
using namespace std;

int main()
{
    char str1[11], str2[11];
    scanf("%s %s", str1, str2);
    printf("%c",str1[0]);
    for(int i = 1; str1[i]; i++)
        if(str1[i] < str2[0]) printf("%c", str1[i]);
        else break;
    printf("%c\n", str2[0]);

    return 0;
}
